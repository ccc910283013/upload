        T.TRAN,
        T.TRANDATE,
        T.TRANDATEHOUR,
        T.TRANDATEMINUTE,
        T.TRANREASON,
        T.TRANORGAN,
        T.TRANORGANCODE,
        T.NEXTCHECK,
        T.NEXTDATE,
        T.NEXTCARES,
        T.NEXTSCREEN,
        T.NEXTSCREENDATE,
        T.NEXTSCREENCARES FROM MEDDOC.PREGNANCY_CARE_V T INNER JOIN PAT_MASTER_INDEX PMI ON T.PATIENT_ID = PMI.PATIENT_ID WHERE
        OUTP_PRE_CHECK_NO = #{_parameter} AND ROWNUM = 1
    </select>
